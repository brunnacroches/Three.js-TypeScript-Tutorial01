{
  "name": "three.js-typescript-tutorial01",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npx webpack serve --config ./src/client/webpack.dev.js --open",
    "start": "node server.js",
    "deploy": "npm run build && npm run deploy:pages",
    "prettier": "prettier ./src --write",
    "lint": "prettier --check \"./src/**/*.{ts,tsx}\" && eslint \"./src/**/*.{ts,tsx}\" && yarn type-check",
      "type-check": "tsc --pretty --noEmit",
      "postinstall": "husky install",
      "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunnacroches/arquivos-3D.git"
  },
  "keywords": [],
  "author": "Brunna Croches",
  "license": "ISC",
  "dependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.134.0",
    "dat.gui": "^0.7.7",
    "three": "^0.135.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "cool-ascii-faces": "^1.3.4",
    "express": "^4.17.2",
    "postinstall": "^0.7.4"
  },
  "setupFiles": [
    "<rootDir>/src/client/client.ts"
  ],
  "testEnvironment": "near-cli/test_environment",
  "testPathIgnorePatterns": [
    "<rootDir>/contract/",
    "<rootDir>/node_modules/"
  ],
  "browserModern": "dist/client/index.html",
  "targets": {
    "browserModern": {
      "engines": {
